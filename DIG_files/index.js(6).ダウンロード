Util.preRegisterComponent("IconTile"),Util.registerComponent("IconTile",(e,t)=>{let{color:i,scale:a,preLoadWidth:o,preLoadHeight:r}={scale:.16,color:"black",preLoadWidth:10,preLoadHeight:10,...t},h=HTML.Canvas({width:o+"px",height:r+"px"});return IconStore.getIconDirect(e).then(e=>{var t=Math.floor(a*e.width),o=Math.floor(a*e.height),t=(h.set({width:t+"px",height:o+"px",canvaswidth:Math.floor(t),canvasheight:Math.floor(o)}),h.getContext("2d"));t.scale(a,a),t.drawImage(e,0,0),t.globalCompositeOperation="source-atop",t.fillStyle=i,t.fillRect(0,0,e.width,e.height),t.globalCompositeOperation="source-over"}),h});