Util.preRegisterComponent("LessonItemTile"),(async()=>{const{Div:l,LocQueriedData:s,Link:a,Span:c}=HTML;await Util.loadComponent("IconTile"),Util.registerComponent("LessonItemTile",(t,e,i,n,o)=>{o=o?s({fn:"student.contenttitle",queryArgs:{id:o},converter:t=>t.title,init:{title:e}}):e;return l.css("activity",n?"activity--dark":null)(Components.IconTile(t),i?a.css("activity__title","activity__title--no-underline")({navigate:i},o):c.css("activity__title")(o))})})();